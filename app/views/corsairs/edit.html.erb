<h1>Corsairs#edit</h1>
<p>Find me in app/views/corsairs/new.html.erb</p>

<%=form_for @corsair do |f| %>
	<%=f.label :first_name, "First name" %><br>
	<%=f.text_field :first_name%><br><br>

	<%=f.label :age, "Age" %><br>
	<%=f.number_field :age%><br><br>

	<%=f.submit "Update" %>

<% end %>

<%= link_to "Delete", @corsair, method: :delete %>